{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2022"
    ],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@acme/shared": [
        "../../packages/shared/src/index.ts"
      ],
      "@acme/schemas": [
        "../../packages/schemas/src/index.ts"
      ],
      "@acme/prompts": [
        "../../packages/prompts/src/index.ts"
      ],
      "@acme/ai": [
        "../../packages/ai/src/index.ts"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
